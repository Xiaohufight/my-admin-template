<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.xiaohu.modules.mapper.SysUserMapper">
    <select id="getSysUserByName" resultType="com.example.xiaohu.modules.entity.SysUser">
        select * from SYS_USER where username = #{username}
    </select>
</mapper>